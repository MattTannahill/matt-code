{
  "name": "matt-code-workspace",
  "type": "module",
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "start": "npm --workspace=packages/cli run start",
    "build": "npm --workspaces run build --if-present",
    "clean": "npm --workspaces run clean --if-present",
    "lint": "npm --workspaces run lint --if-present"
  }
}
