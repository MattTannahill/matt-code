{
  "name": "matt-code-just-bash",
  "description": "Interact with a bash shell from Matt Code",
  "version": "0.0.0",
  "author": "Matt Tannahill",
  "bugs": "https://github.com/MattTannahill/matt-code/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "just-bash": "^2.6.0",
    "matt-code-api": "0.0.0"

  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./lib/index.js",
  "files": [
    "./lib",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/MattTannahill/matt-code",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "oclif": {
    "bin": "matt",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "MattTannahill/matt-code",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "./lib/index.d.ts"
}
